<csw:GetRecords
        xmlns:csw="http://www.opengis.net/cat/csw/2.0.2"
        xmlns:ogc="http://www.opengis.net/ogc"
        service="CSW" version="2.0.2"
        resultType="results"
        startPosition="1"
        maxRecords="100000"
        outputFormat="application/xml"
        outputSchema="http://www.isotc211.org/2005/gmd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:gml="http://www.opengis.net/gml"
        xsi:schemaLocation="http://www.opengis.net/cat/csw/2.0.2 http://schemas.opengis.net/csw/2.0.2/CSW-discovery.xsd">

  <csw:Query typeNames="csw:Record">
    <csw:ElementSetName>full</csw:ElementSetName>
    <csw:Constraint version="1.1.0">
        <ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
            <ogc:Or>
                <ogc:PropertyIsEqualTo>
                    <ogc:PropertyName>LinkageName</ogc:PropertyName>
                    <ogc:Literal>Protected_Areas_CAPAD_2014_Marine</ogc:Literal>
                </ogc:PropertyIsEqualTo>
                <ogc:PropertyIsEqualTo>
                    <ogc:PropertyName>LinkageName</ogc:PropertyName>
                    <ogc:Literal>Protected_Areas_CAPAD_2014_Terrestrial</ogc:Literal>
                </ogc:PropertyIsEqualTo>
                <ogc:PropertyIsEqualTo>
                    <ogc:PropertyName>LinkageName</ogc:PropertyName>
                    <ogc:Literal>World_Heritage_Areas_2007</ogc:Literal>
                </ogc:PropertyIsEqualTo>
                <ogc:PropertyIsEqualTo>
                    <ogc:PropertyName>LinkageName</ogc:PropertyName>
                    <ogc:Literal>Defence_Prohibited_Areas</ogc:Literal>
                </ogc:PropertyIsEqualTo>
                <ogc:PropertyIsEqualTo>
                    <ogc:PropertyName>LinkageName</ogc:PropertyName>
                    <ogc:Literal>Defence_Practice_Areas</ogc:Literal>
                </ogc:PropertyIsEqualTo>
                <ogc:PropertyIsEqualTo>
                    <ogc:PropertyName>LinkageName</ogc:PropertyName>
                    <ogc:Literal>Defence_Training_Areas</ogc:Literal>
                </ogc:PropertyIsEqualTo>
            </ogc:Or>
        </ogc:Filter>
    </csw:Constraint>
  </csw:Query>
</csw:GetRecords>
