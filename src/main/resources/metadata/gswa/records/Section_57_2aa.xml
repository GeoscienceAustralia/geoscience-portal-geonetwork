          <gmd:title>
            <gco:CharacterString>Mineralisation Zones - Designated areas under Section 57 (2aa) Mining Act 1978</gco:CharacterString>
          </gmd:title>
          <gmd:date>
            <gmd:CI_Date>
              <gmd:date>
                <gco:Date>2006-04-26</gco:Date>
              </gmd:date>
              <gmd:dateType>
